CREATE SOURCE src1 TYPE source;
CREATE SOURCE src2 TYPE source;

LOAD STATE st TYPE state TAG default;

CREATE STREAM b1 AS SELECT RSTREAM * FROM src1 [RANGE 1 TUPLES];
CREATE STREAM b2 AS SELECT RSTREAM * FROM src2 [RANGE 1 SECONDS];
CREATE STREAM b3 AS SELECT ISTREAM * FROM b1 [RANGE 1 TUPLES], b2 [RANGE 1 SECONDS] WHERE b1:id = b2:id;

CREATE SINK si TYPE sink;
INSERT INTO si FROM b3;
